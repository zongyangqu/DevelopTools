<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >

    <!--设置圆的半径-->
    <size android:height="31dp" android:width="31dp"/>

    <!--设置圆空心部分的背景色 -->
    <solid android:color="@color/white"/>

    <!--设置圆边的背景色-->
    <stroke
        android:width="2dp"
        android:color="#33da33"/>
</shape>